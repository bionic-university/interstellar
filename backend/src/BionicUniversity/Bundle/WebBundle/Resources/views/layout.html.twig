{% extends '::base.html.twig' %}

{% block body %}
    <nav>
        <ul>
            {% for category in categories() %}
                <li>
                    <a href="{{ path('category_index', {name: category.name}) }}">
                        {{ category.name }}
                    </a>
                </li>
            {% endfor %}
        </ul>
    </nav>
{% endblock %}